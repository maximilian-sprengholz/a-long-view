python3 dep/regexrep.py 'appendix.md' '(subtitle:[\s])(.*)' '\1"Version <b>${CI_COMMIT_TAG}</b>"'

# pandoc call
pandoc --filter pandoc-include --filter pandoc-crossref --citeproc --bibliography=appendix.bib --csl=journal-of-family-research.csl --number-sections --table-of-contents -c ../public/css/vue_extended.css appendix.md -s -o test.html
